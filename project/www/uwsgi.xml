<?xml version="1.0"?>
<uwsgi id="gold-miner">
<app mountpoint="/"></app>
<module>deploy:app</module>
<socket>/tmp/uwsgi.sock</socket>
<processes>4</processes>
<chdir>/var/www/gold-miner</chdir>
<pidfile>/tmp/gold-miner.pid</pidfile>
<daemonize>/var/www/gold-miner/log/gold-miner.log</daemonize>
<stats>/tmp/gold-miner.stats</stats>
<uid>uwsgi</uid>
<gid>www-data</gid>
<chmod-socket>775</chmod-socket>
</uwsgi>
